{
  "manifest_version": 3,
  "name": "X-Fathom",
  "version": "1.0",
  "description": "Enhanced Immersive Experience for Fathom",
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.fathom.video/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "permissions": []
}